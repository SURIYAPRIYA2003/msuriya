
from datetime import date 

class student:
    def __init__(self,name):
        self.name=name
        self.age=18
        
        
    def __str__(self):
        return f'{self.name}'
        
    def __repr__(self):
        return f' student ({self.name})'
        
stu1=student('kutty')
print(stu1)

d=date(year=2022,month=5,day=5)
print(d)
